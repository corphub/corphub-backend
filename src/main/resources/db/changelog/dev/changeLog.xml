<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="PeterA" id="init-dev-db">
        <loadData file="db/changelog/dev/categories.csv"
                  quotchar="'"
                  tableName="category">
        </loadData>
        <loadData file="db/changelog/dev/entries.csv"
                  quotchar="'"
                  tableName="entry">
        </loadData>
        <loadData file="db/changelog/dev/category_entry_link.csv"
                  quotchar="'"
                  tableName="category_entry_link">
        </loadData>
    </changeSet>

</databaseChangeLog>